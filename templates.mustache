<script id="responses" type="text/html">
  {{#answers}}
    <div class="col-xs-6" score="{{score}}" nextId="{{next_id}}"><a class="btn btn-block btn-lg">{{label}}</a></div>
  {{/answers}}
</script>

<script id="next" type="text/html">
  <div class="col-xs-12" nextId="{{next_id}}"><a class="btn btn-block btn-lg btn-success">Next</a></div>
</script>

<script id="question" type="text/html">
  <h1>{{question}}</h1>
</script>
<script id="rnliForm" type="text/html">
  <form >
    Date:<br>
    <input type="date" id="Date" value="26/07/2017">
    ResponseTime(mins):<br>
    <input type="number" id="ResponseTime">
    <br>
    Location:<br>
    <input type="text" id="Location" >
    <br><br>
    Issue:<br>
    <input type="text" id="Issue" >
    Comments:<br>
    <input type="text" id="comments">
    <input type="submit" value="Submit" onClick="rnliForms();">
  </form>

</script>

<script id="audio" type="text/html">
  <audio id="myAudioElement" autoplay="true" loop="true" src="https://c92f6c39-d9b1-4193-b9d8-809ca34108b5:aZmR4CNRBoTo@stream.watsonplatform.net/text-to-speech/api/v1/synthesize?voice=en-US_AllisonVoice&text={{question}}"/>
</script>





<script id="contents" type="text/html">
  <ul class="menu nav nav-pills nav-stacked">
    {{#menuItems}}
      <li role="presentation" id="{{id}}" nextid="{{next_id}}"><a><h2>{{label}}</h2></a></ld>
    {{/menuItems}}
  </ul>
</script>

<script id="evaluation" type="text/html">
  <h1>Patient is {{status}}</h1>
</script>
