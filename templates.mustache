<script id="responses" type="text/html">
  {{#answers}}
    <div class="col-xs-6" score="{{score}}" nextId="{{next_id}}"><a class="btn btn-block btn-lg">{{label}}</a></div>
  {{/answers}}
</script>

<script id="next" type="text/html">
  <div class="col-xs-12" nextId="{{next_id}}"><a class="btn btn-block btn-lg btn-success">Next</a></div>
</script>

<script id="question" type="text/html">
  <h1>{{question}}</h1>
</script>
<script id="patientData" type="text/html">
  <form class="fullWidth">
    <label for="Date">Date:</label>
    <input class="form-control input-lg" type="date" id="Date" value="26/07/2017"><br />

    <label for="ResponseTime">Response Time (minutes):</label>
    <input class="form-control input-lg" type="number" id="ResponseTime"><br />

    <label for="Location">Location:</label>
    <input class="form-control input-lg" type="text" id="Location" ><br />

    <label for="Issue">Issue:</label>
    <textarea  class="form-control input-lg" id="Issue"/><br />

    <label for="comments">Comments:</label>
    <textarea  class="form-control input-lg" id="comments"/><br />
  </form>
</script>

<script id="patientDataSubmit" type="text/html">
  <div class="col-xs-12"><a onClick="rnliForms()" class="btn btn-block btn-lg btn-success">Submit</a></div>
</script>

<script id="audio" type="text/html">
  <audio id="myAudioElement" autoplay="true" src="https://c92f6c39-d9b1-4193-b9d8-809ca34108b5:aZmR4CNRBoTo@stream.watsonplatform.net/text-to-speech/api/v1/synthesize?voice=en-US_AllisonVoice&text={{question}}"/>
</script>

<script id="map" type="text/html">



    <img src="https://maps.googleapis.com/maps/api/staticmap?center=50.743493%2c%20-1.830172&zoom=17&size=350x450&key=AIzaSyBChzxTvbjUlG3UoTwSoAYtqzp8xz1hZqE&callback=initMap">

  </script>





<script id="contents" type="text/html">
